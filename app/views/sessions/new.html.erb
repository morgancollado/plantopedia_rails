<h1>Please Log In</h1> <br>
<%= form_tag sessions_path do %>
<div class="form-group">
    <%= label_tag "Username" %>
    <%= text_field_tag :username %>
</div>

<div class="form-group">
    <%= label_tag "Password" %>
    <%= password_field_tag :password %>
</div>
<div class="form-group">
    <%= submit_tag "Login" %>
</div>
<% end%>

<%= link_to "sign in with github", '/auth/github'  %>